{
  "name": "jsson",
  "displayName": "JSSON - Simplified JSON Transpiler",
  "description": "Syntax highlighting for JSSON - JSON Simplified Object Notation",
  "version": "0.0.2",
  "publisher": "carlosedujs",
  "icon": "assets/sm.png",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "jsson",
    "json",
    "syntax",
    "highlighting"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/carlosedujs/jsson"
  },
  "contributes": {
    "languages": [
      {
        "id": "jsson",
        "aliases": [
          "JSSON",
          "jsson"
        ],
        "extensions": [
          ".jsson"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./assets/sm.svg",
          "dark": "./assets/sm.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "jsson",
        "scopeName": "source.jsson",
        "path": "./syntaxes/jsson.tmLanguage.json"
      }
    ]
  }
  ,
  "scripts": {
    "instructions": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts\\instructions.ps1"
  }
}
